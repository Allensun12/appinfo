        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.sun.dao.DevUserMapper">
    <select id="getDevUser" parameterType="java.lang.String" resultType="cn.sun.pojo.DevUser">
        select * from dev_user where devCode=#{code};
